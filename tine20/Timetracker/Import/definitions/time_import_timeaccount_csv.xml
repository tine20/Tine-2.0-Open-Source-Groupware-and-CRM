<?xml version="1.0" encoding="UTF-8"?>
<config>
    <name>time_import_timeaccount_csv</name>
    <model>Timetracker_Model_Timeaccount</model>
    <plugin>Timetracker_Import_Timeaccount_Csv</plugin>
    <type>import</type>
    <headline>1</headline>
    <dryrun>0</dryrun>
    <delimiter>,</delimiter>
    <extension>csv</extension>
    <mapping>
        <field>
            <source>title</source>
            <destination>title</destination>
        </field>
        <field>
            <source>number</source>
            <destination>number</destination>
        </field>
        <field>
            <source>description</source>
            <destination>description</destination>
        </field>
        <field>
            <source>budget</source>
            <destination>budget</destination>
        </field>
        <field>
            <source>price</source>
            <destination>price</destination>
        </field>
        <field>
            <source>is_open</source>
            <destination>is_open</destination>
        </field>
        <field>
            <source>is_billable</source>
            <destination>is_billable</destination>
        </field>

        <field>
            <source>billed_in</source>
            <destination>billed_in</destination>
        </field>
        <field>
            <source>status</source>
            <destination>status</destination>
        </field>
        <field>
            <source>cleared_at</source>
            <destination>cleared_at</destination>
        </field>
        <field>
            <source>deadline</source>
            <destination>deadline</destination>
        </field>
    </mapping>
</config>
