<?xml version="1.0" encoding="UTF-8"?>
<config>
    <name>time_import_timesheet_csv</name>
    <model>Timetracker_Model_Timesheet</model>
    <plugin>Timetracker_Import_Timesheet_Csv</plugin>
    <type>import</type>
    <headline>1</headline>
    <dryrun>0</dryrun>
    <delimiter>,</delimiter>
    <extension>csv</extension>
    <mapping>
        <field>
            <source>time</source>
            <destination>time</destination>
        </field>
        <field>
            <source>account_id</source>
            <destination>account_id</destination>
        </field>
        <field>
            <source>is_billable</source>
            <destination>is_billable</destination>
        </field>
        <field>
            <source>billed_in</source>
            <destination>billed_in</destination>
        </field>
        <field>
            <source>is_cleared</source>
            <destination>is_cleared</destination>
        </field>
        <field>
            <source>start_date</source>
            <destination>start_date</destination>
        </field>
        <field>
            <source>start_time</source>
            <destination>start_time</destination>
        </field>
        <field>
            <source>duration</source>
            <destination>duration</destination>
        </field>
        <field>
            <source>description</source>
            <destination>description</destination>
        </field>
    </mapping>
</config>
